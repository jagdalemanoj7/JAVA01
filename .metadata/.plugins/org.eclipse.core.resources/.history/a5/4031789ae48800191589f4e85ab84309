package jv19;

public class PalindromeCount {
	
	public static void main(String[] args) {
		
		int num,rem,div,sum=0;		
		
		System.out.println("Palindrome number from 1 to 10 : ");
		for (int i = 1; i <= 10; i++) 
		{
			num=i;
			while(num!=0 || num>0)
			{
				rem = num % 10;
				sum = (sum * 10) + rem;
				div = num / 10;
			}
			
			if(sum==i)
				System.out.print(i+ " ");
			
		}		
	}
}