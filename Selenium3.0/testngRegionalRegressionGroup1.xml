<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <groups>
		<run>
			<include name="SmokeTest"></include>
			<include name="RegressionTest"></include>
		</run>
  </groups>
	
  <test thread-count="5" name="Test">  
    <classes>      
      <class name="testng.CustomerTest">
      		<methods>
      			<include name="createCustomerTest"></include>
      		</methods>
      </class>
    
      <class name="testng.ReportTest">
    		<methods>
    			<include name="deleteReportTest"></include>
    		</methods>
      </class>
    </classes>    
  </test> <!-- Test -->
    
</suite> <!-- Suite -->